      <newsItem id="1207" key="e4767c17-da67-4c68-827c-d5dfbf1a62b3" parentID="1097" level="3" creatorID="0" sortOrder="6" createDate="2017-04-23T17:19:56" updateDate="2017-06-04T20:21:04" nodeName="Irish Lottery Winner Only Has Two Days left To Claim" urlName="irish-lottery-winner-only-has-two-days-left-to-claim" path="-1,1050,1097,1207" isDoc="" nodeType="1087" creatorName="admin" writerName="admin" writerID="0" template="1086" nodeTypeAlias="newsItem">
